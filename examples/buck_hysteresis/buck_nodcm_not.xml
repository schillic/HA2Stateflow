      <flow>il' == (a00c * il + a01c * vc + b0c * Vs) &amp; vc' == (a10c * il + a11c * vc + b1c * Vs)</flow>
    </location>
    <location id="3" name="discharging" x="1695.5" y="449.5" width="641.0" height="224.0">
